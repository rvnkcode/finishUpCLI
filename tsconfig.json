{
    "compilerOptions": {
        "module": "CommonJS",
        "moduleResolution": "Node",
        "target": "ES5",
        "sourceMap": false,
        //여기서부터 내가 추가한 설정들
        "allowJs": true,
        "checkJs": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "removeComments": true,
        //"outDir": "./js"
    },
    "exclude": [
        "node_modules",
        "doc"
    ]
}